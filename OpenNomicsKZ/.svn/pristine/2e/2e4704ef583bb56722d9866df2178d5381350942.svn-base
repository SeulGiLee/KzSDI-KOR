<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>	
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
    <typeAliases>
    	<!-- 사용자 관련 -->
    	<typeAlias alias="User" type="opennomics.com.main.user.domain.UserVO"/>
    	<!-- 접수요청 관련 -->
    	<typeAlias alias="Receipt" type="opennomics.com.main.receipt.domain.ReceiptVO"/>
    	<!-- 에러리포트 관련 -->
    	<typeAlias alias="errReport" type="opennomics.com.main.qa.domain.QAErrReportVO"/>
    </typeAliases>
    
</configuration>
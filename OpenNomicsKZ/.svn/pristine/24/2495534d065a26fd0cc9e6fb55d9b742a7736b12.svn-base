/*main rolling*/
#touchSlider {width:100%; min-width:940px; min-height:768px; margin:0 auto; background:#1c1c1c;  position:relative;}
#touchSlider ul { width:99999px; min-height:768px; position:absolute; top:0; left:0; overflow:hidden; }
#touchSlider ul li {float:left; width:100%; min-height:768px; background:#9C9; background-size:auto 1024px; font-size:14px; color:#333; }
#touchSlider ul li .show{width:900px; height:320px; display:inline-block; margin:18% auto;}
#touchSlider ul li.bgheight{height:100%;}
#touchSlider ul li.bgwidth{width:100%;}
.btn_area {position:absolute; top:0; left:50%; z-index:990; margin:0 0 0 -579px; width:1158px; height:106px; background:none; overflow:hidden;}
.btn_area button { display:block; width:56px; height:106px; background:#000; font-size:16px; color:#fff; font-weight:bold; }
.btn_area button.btn_prev {position:absolute; top:0; left:0; width:56px; height:106px; font-size:0; text-indent:-100000px; background:url("../images/kor/main_btn_rolling.png") no-repeat left top;}
.btn_area button.btn_next {position:absolute; top:0; right:0; width:56px; height:106px; font-size:0; text-indent:-100000px; background:url("../images/kor/main_btn_rolling.png") no-repeat right top;}
.btn_area button.btn_prev:hover{background-position:left -106px;}
.btn_area button.btn_next:hover{background-position:right -106px;}

.btn_area2{position:absolute;top:0; left:50%; margin:0 0 0 -579px; width:1158px; height:50px; z-index:999;text-align:center;}
.btn_area2 .btn_page { display:inline-block; width:12px; height:12px; margin:3px; font-size:.1%; line-height:.1%; text-indent:-9999px; background:url("../images/kor/main_select_bg.png") no-repeat center center; }
.btn_area2 .btn_page.on { background:url("../images/kor/main_select_on.png") no-repeat center center;}

.sliderStopPlay {position:absolute; left:50%; width:900px; margin-left:-450px; text-align:left; z-index:999;}
.sliderStopPlay button {width:16px; height:16px;}
.sliderStopPlay button::-moz-focus-inner {padding:0; border:0;}
.sliderStopPlay button:focus {outline:1px dotted #de1620;}
.sliderStopPlay span {display:inline-block; .display:inline; color:#a6a6a6; font-size:14px; .zoom:1;}
.sliderStopPlay button.stop {background:url("../images/kor/main_btn_stop.png") no-repeat 50% 0;}
.sliderStopPlay button.play {background:url("../images/kor/main_btn_play.png") no-repeat 50% 0;}
.sliderStopPlay button.stop:hover,
.sliderStopPlay button.stop:active,
.sliderStopPlay button.stop:focus,
.sliderStopPlay button.stop.on {background:url("../images/kor/main_btn_stop_on.png") no-repeat 50% 0;}
.sliderStopPlay button.play:hover,
.sliderStopPlay button.play:active,
.sliderStopPlay button.play:focus,
.sliderStopPlay button.play.on {background:url("../images/kor/main_btn_play_on.png") no-repeat 50% 0;}

